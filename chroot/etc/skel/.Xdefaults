! Fonts
Xft.dpi: 96
Xft.hinting: true
Xft.rgba: rgb
Xft.autohint: false
Xft.hintstyle: hintslight
Xft.lcdfilter: lcddefault

URxvt.font: xft:IBMPlexMono:pixelsize=15, xft:IosevkaNerdFontMono:pixelsize=12, xft:NotoSansMonoCJKJP:pixelsize=12, xft:Apple Color Emoji:pixelsize=12
URxvt.scrollBar: false
URxvt.cursorBlink: true
URxvt.depth: 32
URxvt.url-launcher: /usr/bin/firefox
URxvt.perl-ext-common: default,matcher,tabbedex
URxvt.matcher.button: 1
URxvt.cursorUnderline: true
URxvt.internalBorder: 6
URxvt.letterSpace: 0
URxvt.lineSpace: 0

URxvt.tabbed.new-button: false
URXvt.tabbed.reopen-on-close: yes

URxvt.transparent: false
!URxvt.shading: 10
!URxvt.foreground: #FFFFFF
!URxvt.background: #000000

!Use Control-Shift-V/C for X clipboard
URxvt.iso14755: false
URxvt.keysym.Shift-Control-V: eval:paste_clipboard
URxvt.keysym.Shift-Control-C: eval:selection_to_clipboard

! PaperColor Theme light
URxvt*cursorColor: #4D4D4C
URxvt*pointerColor: #4D4D4C
URxvt.foreground: #4D4D4C
URxvt.background: #EEEEEE
! black
URxvt.color0: #D0D0D0
URxvt.color8: #737373
! red
URxvt.color1: #D7005F
URxvt.color9: #D7005F
! green
URxvt.color2: #718C00
URxvt.color10: #718C00
! yellow / orange
URxvt.color3: #D75F00
URxvt.color11: #D75F00
!blue
URxvt.color4: #4271AE
URxvt.color12: #4271AE
! magenta
URxvt.color5: #8959A8
URxvt.color13: #8959A8
! cyan
URxvt.color6: #3E999F
URxvt.color14: #3E999F
! white
URxvt.color7: #4D4D4C
URxvt.color15: #F5F5F5

URxvt.backgroundPixmap: ~/.config/urxvt/lines.png;style=tiled
